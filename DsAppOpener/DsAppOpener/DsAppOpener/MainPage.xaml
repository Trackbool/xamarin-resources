<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:DsAppOpener"
             x:Class="DsAppOpener.MainPage">

    <StackLayout>
        <Button x:Name="openFacebookApp" Clicked="OpenFacebookApp_Clicked" Text="Open FB" />
        <Button x:Name="openInstagramApp" Clicked="OpenInstagramApp_Clicked" Text="Open IG" />
        <Button x:Name="openYoutubeApp" Clicked="OpenYoutubeApp_Clicked" Text="Open YT" />
    </StackLayout>

</ContentPage>
